package main //пакет программы
import (  //импорт пакетов
	"fmt" //вывод текста в консоль
	"time" //функции работы с временем и датой
)
func main() { // Является обязательной, держит код программ.
	fmt.Println("Текущая дата и время:", time.Now()) //преобразует данные в строку, вывод в консоль. возвращает текущие дату и время 
} 

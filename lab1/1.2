package main
import "fmt"
func main() {
    var intVar int = 63 //объявление переменной целого числа
    var floatVar float64 = 3.14 //числа с плавающей точкой 
    var strVar string = "ура работает)" //символов в кодировке UTF-8
    var boolVar bool = true //логический тип, может принимать значения true или false
    fmt.Println("int =", intVar) //вывод в консоль. 1 штука добавляет пробелы.2 целые числа
    fmt.Println("float64 =", floatVar) //вещественного числа
    fmt.Println("string =", strVar) //строки
    fmt.Println("bool =", boolVar) //логического значения
}
